{
  "ConnectionStrings": {
    "Entities": "#{ConnectionStrings_Entities}#"
  },
  "Cors": {
    "Origin": "#{Cors.Origin}#",
    "AllowedHeaders": "*",
    "AllowedMethods": "*",
    "AllowCredentials": true,
    "ExposedHeaders": "#{Cors.ExposedHeaders}#"
  },
  "Identity": {
    "Authority": "#{Identity_Authority}#",
    "Audience": "#{Identity_Audience}#",
    "ValidateAudience": "#{Identity_ValidateAudience}#",
    "ClientId": "#{Identity_ClientId}#",
    "ClientSecret": "#{Identity_ClientSecret}#",
    "Scope": "#{Identity_Scope}#",
    "GetClaimsFromUserInfoEndpoint": "#{Identity_GetClaimsFromUserInfoEndpoint}#",
    "ClaimPrefix": "#{Identity_ClaimPrefix}#"
  },
  "QuartzCron": {
    "CalcularIndicadoresEvaluacion": "#{QuartzCron.CalcularIndicadoresEvaluacion}#",
    "EnviarEvaluacionProveedores": "#{QuartzCron.EnviarEvaluacionProveedores}#"
  },
  "SmtpConfig": {
    "Sender": "erptqnotificaciones@tqgrupo.com",
    "SenderAlias": "Notificaciones",
    "Subject": "(Dev) Eficacia de Proveedores",
    "Host": "#{SmtpConfig.Host}#",
    "Port": 25,
    "EnableSsl": false,
    "Username": "#{SmtpConfig.Username}#",
    "Password": "#{SmtpConfig.Password}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
